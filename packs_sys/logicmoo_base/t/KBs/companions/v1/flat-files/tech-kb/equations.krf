(in-microtheory MetaengineeringEquationsMt)

(genlMt MetaengineeringEquationsMt MetaengineeringUnitsMt)

;;; Equations should also reference quantities and units.

(isa EquationType Collection)

(isa FramingEquation EquationType)
(isa BridgingEquation EquationType)
(isa InternalEquation EquationType)

(isa equationType BinaryRelation)
(arg1Isa equationType Innovation)
(arg2Isa equationType EquationType)
(comment equationType "Characterizes an equation by its type (Framing, Bridging, or Internal).")

(isa contentML BinaryRelation)
(arg1Isa contentML Innovation)
(arg2Isa contentML CharacterString)
(comment contentML "Associates the MathML content markup for an equation with the Innovation (principle, law, rule) it describes.")

(isa presentationML BinaryRelation)
(arg1Isa presentationML Innovation)
(arg2Isa presentationML CharacterString)
(comment presentationML "Associates the MathML presentation markup for an equation with the Innovation (principle, law, rule) it describes.")


(equationType AmperesLaw FramingEquation)  ; Framing Equation
(contentML AmperesLaw
"<math> 
  <apply>
    <eq/>
    <ci>F</ci>
    <apply>
      <divide/>
      <apply>
        <times/>
        <ci>B</ci>
        <ci>I</ci>
        <ci>L</ci>
        <apply>
         <sin/>
         <ci>&#x003B8;</ci>
        </apply>
      </apply>
      <cn>10</cn>
    </apply>
  </apply> 
</math>")
(presentationML AmperesLaw
"<?xml version=""1.0"" encoding=""UTF-8""?>
<!DOCTYPE math SYSTEM ""dtd/mathml2.dtd"">
<math xmlns=""http://www.w3.org/1998/Math/MathML"" mode=""display"">
  <mrow>
    <mi>F</mi>
    <mo>=</mo>
    <mrow>
      <mfrac>
        <mrow>
          <mi>V</mi>
          <mo>&InvisibleTimes;</mo>
          <mi>Q</mi>
          <mo>&InvisibleTimes;</mo>
          <mi>B</mi>
          <mo>&InvisibleTimes;</mo>
          <mi>sin</mi>
          <mo>&ApplyFunction;</mo>
          <mi>&alpha;</mi>
        </mrow>
        <mrow>
          <mn>10</mn>
        </mrow>
      </mfrac>
    </mrow>
  </mrow>
</math>")

(equationType BernoullisTheorem FramingEquation)
(contentML BernoullisTheorem
"<math> 
  <apply>
    <eq/>
    <ci>H</ci>
    <apply>
      <plus/>
      <ci>h</ci>
      <apply>
        <divide/>
        <ci>P</ci>
        <apply>
          <times/>
           <ci>&#x003C1;</ci>
           <ci>g</ci>
        </apply>
      </apply>
      <apply>
        <divide/>
        <apply>
          <power/>
          <ci>v</ci>
          <cn>2</cn>
        </apply>
        <apply>
          <times/>
          <cn>2</cn>
          <ci>g</ci>
        </apply>
      </apply>
    </apply>
  </apply> 
</math>")
(presentationML BernoullisTheorem
"<?xml version=""1.0"" encoding=""UTF-8""?>
<!DOCTYPE math SYSTEM ""dtd/mathml2.dtd"">
<math xmlns=""http://www.w3.org/1998/Math/MathML"" mode=""display"">
  <mrow>
    <mi>H</mi>
    <mo>=</mo>
    <mrow>
      <mrow>
        <mi>h</mi>
        <mo>+</mo>
        <mfrac>
          <mrow>
            <mi>P</mi>
          </mrow>
          <mrow>
            <mi>&rho;</mi>
            <mo>&InvisibleTimes;</mo>
            <mi>g</mi>
          </mrow>
        </mfrac>
      </mrow>
      <mo>+</mo>
      <mrow>
        <mfrac>
          <mrow>
            <msup>
              <mi>v</mi>
              <mn>2</mn>
            </msup>
          </mrow>
          <mrow>
            <mn>2</mn>
            <mo>&InvisibleTimes;</mo>
            <mi>g</mi>
          </mrow>
        </mfrac>
      </mrow>
    </mrow>
  </mrow>
</math>")

