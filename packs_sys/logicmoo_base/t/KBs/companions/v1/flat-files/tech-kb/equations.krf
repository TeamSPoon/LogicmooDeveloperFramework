(contentML SchrodingersWaveEquation
"<math> 
  <apply>
    <eq/>
    <apply>
      <plus/>
      <apply>
        <partialdiff/>
        <bvar>
          <ci>r</ci>
          <degree><cn>2</cn></degree>
        </bvar>
        <ci>&#x003C8;</ci>
      </apply>
      <apply>
        <times/>
        <apply>
      <divide/>
      <apply>
        <times/>
        <cn>8</cn>
        <apply>
          <power/>
          <ci>&#x003C0;</ci>
          <cn>2</cn>
        </apply>
        <ci>m</ci>
      </apply>
      <apply>
        <power/>
        <ci>h</ci>
        <cn>2</cn>
      </apply>
        </apply>
        <apply>
          <plus/>
          <ci>E</ci>
          <apply>
            <divide/>
            <apply>
              <times/>
              <ci>Z</ci>
              <apply>
                <power/>
                <ci>e</ci>
                <cn>2</cn>
              </apply>
            </apply>
            <ci>r</ci>
          </apply>
        </apply>
        <ci>&#x003C8;</ci>
      </apply>
    </apply>
    <cn>0</cn>
  </apply> 
</math>")

(equationType SnellsLaw FramingEquation)
(presentationML SnellsLaw
"<?xml version=""1.0"" encoding=""UTF-8""?>
<!DOCTYPE math SYSTEM ""dtd/mathml2.dtd"">
<math xmlns=""http://www.w3.org/1998/Math/MathML"" mode=""display"">
  <mrow>
    <mfrac>
        <msub>
          <mi>v</mi>
          <mi>i</mi>
        </msub>
        <msub>
          <mi>v</mi>
          <mi>r</mi>
        </msub>
    </mfrac>
    <mo>=</mo>
      <mfrac>
        <mrow>
          <mi>sin</mi>
          <mo>&ApplyFunction;</mo>
          <msub>
            <mi>&theta;</mi>
            <mi>i</mi>
          </msub>
        </mrow>
        <mrow>
          <mi>sin</mi>
          <mo>&ApplyFunction;</mo>
          <msub>
            <mi>&theta;</mi>
            <mi>r</mi>
          </msub>
        </mrow>
      </mfrac>
    <mo>=</mo>
    <mfrac>
        <msub>
          <mi>&eta;</mi>
          <mi>i</mi>
        </msub>
        <msub>
          <mi>&eta;</mi>
          <mi>r</mi>
        </msub>
    </mfrac>
  </mrow>
</math>")

