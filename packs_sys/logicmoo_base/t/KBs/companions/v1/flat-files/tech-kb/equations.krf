(equationType NewtonsLawOfUniversalGravitation FramingEquation)
(contentML NewtonsLawOfUniversalGravitation
"<math> 
  <apply>
    <eq/>
    <ci>g</ci>
    <apply>
      <times/>
      <ci>k</ci>
      <apply>
        <divide/>
        <apply>
          <times/>
          <ci>
            <msub>
             <mi>m</mi>
             <mi>1</mi>
            </msub>
          </ci>
          <ci>
            <msub>
             <mi>m</mi>
             <mi>2</mi>
            </msub>
          </ci>
        </apply>
        <apply>
          <power/>
          <ci>r</ci>
          <cn>2</cn>
        </apply>
      </apply>
    </apply>
  </apply> 
</math>")
