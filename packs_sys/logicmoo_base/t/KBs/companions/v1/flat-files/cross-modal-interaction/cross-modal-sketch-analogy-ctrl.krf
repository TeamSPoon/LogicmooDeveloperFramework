(notForAnalogy computeShapeAttributesFor)
(notForAnalogy computeShapeRelationsFor)
(notForAnalogy entityTypesLastModifiedTime)
(notForAnalogy forceLastModifiedTime)
(notForAnalogy inkLastModifiedTime)
(notForAnalogy nameStringLastModifiedTime)
(notForAnalogy nuSketchCaseID)
(notForAnalogy sketchFor)
(notForAnalogy subSketchFor)
(notForAnalogy subSketchGroupFor)
(notForAnalogy subSketchHasPose)
(notForAnalogy subSketchHasGenre)
(notForAnalogy voronoiFor)
(notForAnalogy areaOfObject)
(notForAnalogy csysLastModifiedTime)
(notForAnalogy needVisualPositionalRelation)
(notForAnalogy shapeRelationsComputed)
(notForAnalogy directionalSignature)
(notForAnalogy rcc8-DC)
(notForAnalogy rcc8-EQ)
(notForAnalogy uninferredSentence)
(notForAnalogy q-2D-orientation)
(notForAnalogy glyphRepresentsObject)
(notForAnalogy genlMt)
(notForAnalogy activeSketchObject)
(notForAnalogy hasRCC8Relation)
(notForAnalogy nameString)


;;;Glyph Attributes which are not used
