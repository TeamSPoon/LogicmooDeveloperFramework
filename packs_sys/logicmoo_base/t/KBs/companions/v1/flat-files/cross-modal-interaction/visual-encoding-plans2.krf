(in-package :cl-user)

(in-microtheory VisualEncodingPlansMt)

(isa VisualEncodingPlansMt (ExperimentInfraStructureFn "Cross Modal Interaction"))


(preconditionForMethod
 (and (sameVisualEncodedObject ?object ?same-object)
      ;;;;this may not be needed
      (lookupOnly (wmOnly (ist-Information (EncodedCaseFn ?same-object SUB-EP-ID) ?fact)))) 
  (methodForAction
   (encodeVisually ?object)
   (actionSequence
    (TheList
     (doAnnounce "There is a prior encoding availabled for object ~A" (?object))
     ;;ontologize this and try justify if applicatble
     (doTell (encodingForObject ?object (EncodedCaseFn ?same-object SUB-EP-ID)))))))

