(in-microtheory CompanionsMt)


(isa dehydrateSME Predicate)
(arity dehydrateSME 2)
(arg1Isa dehydrateSME String) ;; The path to put it in
(arg2Isa dehydrateSME Mapping)

(isa storeSMEMapping Predicate)
(arity storeSMEMapping 2)
(arg1Isa storeSMEMapping String) ;; The path to put it in
(arg2Isa storeSMEMapping Mapping)

(isa AtFn Function-Denotational)

(arity AtFn 2)
(arg1Isa AtFn TemporalThing)
(arg2Isa AtFn Situation)
(resultIsa AtFn TemporalThing)
(comment AtFn "(AtFn ?thing ?situation) is ?thing, at the period
?situation.")

(in-microtheory UniversalVocabularyMt)
(ubiquitousForAnalogy AtFn)

