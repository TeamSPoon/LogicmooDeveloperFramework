;;; For Q 370

;; the boiling of water creates steam
(in-microtheory (CommonsenseMicrostoryMtFn BoilingWaterSteams))
(genlMt (CommonsenseMicrostoryMtFn BoilingWaterSteams) CommonsenseMicrostoryMt)

(isa water1983 (LiquidFn Water))
(isa boil4837 Boiling)
(causes-PropProp (and (isa water1983 (LiquidFn Water))
                     (isa boil4837 Boiling)
                     (inputsDestroyed boil4837 water1983))
                (createdBy steam1984 boil4837))
(inputsDestroyed boil4837 water1983)
(isa steam1984 (GaseousFn Water))
(createdBy steam1984 boil4837)

(in-microtheory MatchConstraintStorageMt)
(matchConstraintsFor (CommonsenseMicrostoryMtFn BoilingWaterSteams) (requireWithinPartitionCorrespondences Boiling))


;; boiling water in a container heats the container
(in-microtheory (CommonsenseMicrostoryMtFn BoilingWaterHeatsItsContainer))
(genlMt (CommonsenseMicrostoryMtFn BoilingWaterHeatsItsContainer) CommonsenseMicrostoryMt)

(isa water1979 (LiquidFn Water))
(isa boil4836 Boiling)
(inputsDestroyed boil4836 water1979)
(in-UnderspecifiedContainer water1979 container5774)
(isa heat58972 HeatingProcess)
(objectOfStateChange heat58972 container5774)
(causes-PropProp (and (isa water1979 (LiquidFn Water))
                      (isa boil4836 Boiling)
                      (inputsDestroyed boil4836 water1979)
                      (in-UnderspecifiedContainer water1979 container5774))
                 (and (isa heat58972 HeatingProcess)
                      (objectOfStateChange heat58972 container5774)))
(createdBy steam1984 boil4837)

(in-microtheory MatchConstraintStorageMt)
(matchConstraintsFor (CommonsenseMicrostoryMtFn BoilingWaterHeatsItsContainer) (requireWithinPartitionCorrespondences Boiling))
(matchConstraintsFor (CommonsenseMicrostoryMtFn BoilingWaterHeatsItsContainer) (requireWithinPartitionCorrespondences HeatingProcess))

;;;(requiredBaseCorrespondence container5774)

;; boiling water in a container creates steam in that container
(in-microtheory (CommonsenseMicrostoryMtFn SteamIsInItsWatersContainer))
(genlMt (CommonsenseMicrostoryMtFn SteamIsInItsWatersContainer) CommonsenseMicrostoryMt)

(isa water1986 (LiquidFn Water))
(in-UnderspecifiedContainer water1986 container5775)
(isa boil4838 Boiling)
(inputsDestroyed boil4838 water1986)
(isa steam1985 (GaseousFn Water))
(createdBy steam1985 boil4838)
(in-UnderspecifiedContainer steam1985 container5775)
(causes-PropProp (and (isa water1979 (LiquidFn Water))
                      (isa boil4836 Boiling)
                      (inputsDestroyed boil4836 water1979)
                      (in-UnderspecifiedContainer water1979 container5774))
                 (and (isa steam1985 (GaseousFn Water))
                      (createdBy steam1985 boil4838)
                      (in-UnderspecifiedContainer steam1985 container5775)))

(in-microtheory MatchConstraintStorageMt)
(matchConstraintsFor (CommonsenseMicrostoryMtFn SteamIsInItsWatersContainer) (requireWithinPartitionCorrespondences Boiling))

;;;(requiredBaseCorrespondence container5775)
