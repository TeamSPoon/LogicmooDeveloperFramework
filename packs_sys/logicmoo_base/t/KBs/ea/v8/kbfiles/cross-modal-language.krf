;;sem transes
(in-microtheory EANLU :exclude-globals t)

(compoundSemTrans Go-TheWord (TheList "with") Verb TransitiveNPFrame
                  (compatibleWithIn :SUBJECT :OBJECT Script-Underspecified))

(compoundSemTrans Go-TheWord (TheList "best" "with") Verb TransitiveNPFrame
                  (compatibleWithIn :SUBJECT :OBJECT Script-Underspecified))

(adverbSemTrans Why-TheWord 0 FrameForAdverbs 
                (objectOfEvent :ACTION (Why-ReasonFor :CLAUSE)))


;;lexicon
(in-microtheory ComlexMt :exclude-globals t)

(definitionInDictionary COMLEX31Lexicon "why" (why (adverb (orth "why") (root why1) (wh q)) why1))




