(in-microtheory NuSketchMt)

(isa MappingDisplayColor SecondOrderCollection)
(genls MappingDisplayColor ChromaticColor)
(comment MappingDisplayColor
 "The collection of colors that are used to display analogical mappings in
  NuSketch.")

(isa OrangeColor MappingDisplayColor)
(isa GreenColor MappingDisplayColor)
(isa BlueColor MappingDisplayColor)
(isa PurpleColor MappingDisplayColor)
(isa BrilliantBlue-Color MappingDisplayColor)
